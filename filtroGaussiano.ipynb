    "import glob\n",
    "import cv2\n",
    "\n",
    "images = [cv2.imread(file) for file in glob.glob(\"imagenesOriginales/*.jpg\")]\n",
    "j=1\n",
    "for i in images:\n",
    "    blur = cv2.GaussianBlur(i,(17,17),0)\n",
    "    cv2.imwrite('imagenesBlur/00goose-mugshot-'+'%d'%j + '.jpg',blur)\n",
    "    j = j+1"
   ]
  }
